table><pre lang=3D"c++">mem[ current_ImageBase + 0x1000 ] =3D=20
   mem[ current_ImageBase + 0x1000 ] + delta_ImageBase ;
mem[ current_ImageBase + 0x1016 ] =3D=20
   mem[ current_ImageBase + 0x1016 ] + delta_ImageBase ;
mem[ current_ImageBase + 0x1028 ] =3D=20
